<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <changeSet id="init_script_01.000.00"  runAlways="false" runOnChange="true" author="krasilova">
        <sqlFile path="schema.sql" relativeToChangelogFile="true" stripComments="true"/>
        <rollback>
            <sqlFile path="rollback.sql" relativeToChangelogFile="true" stripComments="true"/>
        </rollback>
    </changeSet>

</databaseChangeLog>